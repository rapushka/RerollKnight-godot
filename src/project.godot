[dotnet]

project/assembly_name="RerollKnight"

[file_customization]

folder_colors={
"res://UI/": "pink",
"res://addons/": "orange"
}

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
